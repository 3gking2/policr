<html class="has-navbar-fixed-top">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <!-- Chrome, Firefox OS and Opera -->
    <meta name="theme-color" content="#29D1ED" />
    <!-- Windows Phone -->
    <meta name="msapplication-navbutton-color" content="#29D1ED" />
    <!-- iOS Safari -->
    <meta name="apple-mobile-web-app-status-bar-style" content="#29D1ED" />
    <title>POLICR</title>
    <link
      rel="stylesheet"
      href="<%= Digests.logical_path(%{/css/user.css}) %>"
    />
  </head>

  <body>
    <div id="app"></div>
    <script>
      BOT_USERNAME = "test";
    </script>
    <script
      type="text/javascript"
      src="<%= Digests.logical_path(%{/js/user.js}) %>"
    ></script>
    <style>
      /* latin-ext */
      @font-face {
        font-family: "Aleo";
        font-style: normal;
        font-weight: 400;
        font-display: swap;
        src: local("Aleo Regular"), local("Aleo-Regular"),
          url(<%=Digests.logical_path("/fonts/Aleo-latin-ext.woff2")%>)
            format("woff2");
        unicode-range: U+0100-024F, U+0259, U+1E00-1EFF, U+2020, U+20A0-20AB,
          U+20AD-20CF, U+2113, U+2C60-2C7F, U+A720-A7FF;
      }
      /* latin */
      @font-face {
        font-family: "Aleo";
        font-style: normal;
        font-weight: 400;
        font-display: swap;
        src: local("Aleo Regular"), local("Aleo-Regular"),
          url(<%=Digests.logical_path("/fonts/Aleo-latin.woff2")%>)
            format("woff2");
        unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6,
          U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2122, U+2191, U+2193,
          U+2212, U+2215, U+FEFF, U+FFFD;
      }
    </style>
  </body>
</html>
